pipeline {
    agent any

    stages {
        stage('dev') {
            steps {
                echo 'appilication is building'
            }
        }
         stage('testing') {
            steps {
                echo 'automation testing'
            }
        }
         stage('UAT') {
            steps {
                echo 'waiting for user acceptancy'
            }
        }
         stage('release') {
            steps {
                echo 'appilication is ready to release and to productive service'
            }
        }
        
    }
}
